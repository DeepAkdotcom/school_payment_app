# Copy this file to .env and fill in values before running locally or deploying

# PostgreSQL connection string (use Neon/Supabase/ElephantSQL etc.)
# Example (Neon): postgres://USER:PASSWORD@HOST/dbname
DATABASE_URL=postgres://USER:PASSWORD@HOST/DB_NAME

# App environment
NODE_ENV=production

# The server listens on this port (hosts typically inject PORT automatically)
PORT=8080
